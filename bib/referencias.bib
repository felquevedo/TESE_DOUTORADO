	keywords = {Analytical solution,Numerical modeling,Supported tunneling,Tunnel excavation,Viscoplasticity},
	pages = {123--132},
	title = {{Analytical and numerical approach for tunnel face advance in a viscoplastic rock mass}},
	Urlaccessdate = {15 mar. 2021},
