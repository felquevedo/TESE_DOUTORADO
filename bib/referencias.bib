	journal = {Tunnelling and Underground Space Technology},
	keywords = {Claystones,Creep,Lemaitre's model,Mont Terri,Numerical modelling,Rheology,Scale effects,Tunnel},
	month = {oct},
	number = {4},
	pages = {415--424},
	title = {{Back analysis of time-dependent behaviour of a test gallery in claystone}},
	Urlaccessdate = {29 mar. 2021},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0886779802000664},
