	journal = {International Journal for Numerical and Analytical Methods in Geomechanics},
	keywords = {Analytical solution,Cavity convergence,Laplace transform,Numerical inversion,Poro-visco-elasticity},
	month = {oct},
	number = {14},
	pages = {1737--1761},
	title = {{Analytical study of mine closure behaviour in a poro-visco-elastic medium}},
	Urlaccessdate = {26 mar. 2021},
