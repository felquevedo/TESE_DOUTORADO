	journal = {Transactions of Tianjin University},
	keywords = {3D finite element method,construction,numerical analysis,simulation,tunnel boring machine,：water conveyance tunnel},
	pages = {101--107},
	title = {{3D Finite Element Simulation of Tunnel Boring Machine Construction Processes in Deep Water Conveyance Tunnel}},
	Urlaccessdate = {15 mar. 2021},
