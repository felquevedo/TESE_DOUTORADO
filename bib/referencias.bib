	file = {:C\:/Users/fel_q/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bernaud - 1991 - Tunnels profonds dans les milieux viscoplastiques approches exp{\'{e}}rimentale et num{\'{e}}rique.pdf:pdf},
