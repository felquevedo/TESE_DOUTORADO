@article{Alejano2009,
	abstract = {Rock mass behaviour model selection and, in particular, rock mass post-failure behaviour are key issues in analysing tunnel stability, in particular in terms of the correct application of design techniques such as the convergence-confinement method and also numerical modelling. Three different quality rock masses (good, average and poor) were defined in which simulated standard tunnels were excavated. Different behaviours - including elastic perfectly plastic, elastic brittle and three strain softening behaviours - were modelled for each type of rock mass and increasingly realistic parameters were calculated, along with the corresponding ground reaction curves. The results obtained demonstrate the importance of adequate post-failure behaviour model selection for tunnel analysis. Also assessed are the effects of the standard support and reinforcement. {\textcopyright} 2009 Elsevier Ltd. All rights reserved.},
	author = {Alejano, L. R. and Rodriguez-Dono, A. and Alonso, E. and Fdez.-Man{\'{i}}n, G.},
	doi = {10.1016/j.tust.2009.07.004},
	file = {:C\:/Users/fel_q/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Alejano et al. - 2009 - Ground reaction curves for tunnels excavated in different quality rock masses showing several types of post-fail.pdf:pdf},
	issn = {08867798},
	journal = {Tunnelling and Underground Space Technology},
	keywords = {Ground reactions curves,Post-failure behaviour,Strain softening},
	month = {nov},
	number = {6},
	pages = {689--705},
	publisher = {Pergamon},
	title = {{Ground reaction curves for tunnels excavated in different quality rock masses showing several types of post-failure behaviour}},
	Urlaccessdate = {24 mar. 2021},
	url = {https://www.sciencedirect.com/science/article/pii/S0886779809000686},
	volume = {24},
	year = {2009}
}

