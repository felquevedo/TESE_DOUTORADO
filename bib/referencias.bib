	issn = {14359529},
	journal = {Bulletin of Engineering Geology and the Environment},
	keywords = {3D numerical modeling,FLAC3D,Surface settlement,Tunnel interaction},
	number = {3},
	pages = {439--448},
	Urlaccessdate = {15 mar. 2021},
	url ={https://link.springer.com/article/10.1007/s10064-010-0333-8},
	title = {{Analysis of interaction between tunnels in soft ground by 3D numerical modeling}},
