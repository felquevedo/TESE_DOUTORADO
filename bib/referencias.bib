	keywords = {Brickwork,Masonry,Mathematical modelling,Tunnelling,Tunnels},
	number = {1},
	pages = {17--29},
	title = {{Modelling tunnellmg-induced settlement of masonry buildings}},
	Urlaccessdate = {15 mar. 2021},
	url = {https://www.icevirtuallibrary.com/doi/full/10.1680/geng.2000.143.1.17},
