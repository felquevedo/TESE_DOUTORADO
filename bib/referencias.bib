	isbn = {0886-7798},
	issn = {08867798},
	journal = {Tunnelling and Underground Space Technology},
	keywords = {Design nomogram,Numerical investigation,Shield jamming,Squeezing ground,Tunnel boring machine},
	number = {4},
	pages = {433--455},
	publisher = {Elsevier Ltd},
	title = {{Thrust force requirements for TBMs in squeezing ground}},
	Urlaccessdate = {09 mar. 2021},
	url = {http://dx.doi.org/10.1016/j.tust.2010.02.008},
	volume = {25},
