	journal = {Rock Mechanics and Rock Engineering},
	keywords = {Anisotropy,Monitoring,Squeezing ground,Time-dependent behavior,Tunnel},
	month = {nov},
	number = {6},
	pages = {2301--2317},
	title = {{Anisotropic Time-Dependent Modeling of Tunnel Excavation in Squeezing Ground}},
